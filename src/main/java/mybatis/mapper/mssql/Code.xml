<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.enixone.enixClever.cms.was.dao.CodeDao">

    <!-- -->
    <select id="selectPosition" resultMap="commonSql.Code">
		SELECT
			CODE_NAME, 
			CODE
	 	FROM
			EC_CODE
		WHERE CODE_GROUP = 'POSITION'
	
    </select>
   
</mapper>